<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<!-- Response Error Helper -->
	<bean id="responseErrorHelper" class="com.kiesoft.helperimpl.response.DefaultResponseErrorHelper" />
	
	<!-- Validator Helper -->
	<bean id="validatorHelper" class="com.kiesoft.helperimpl.validator.DefaultValidatorHelper" />
	
	<!-- User Service -->
	<bean id="userDTOService" class="com.kiesoft.serviceimpl.user.DefaultUserDTOService" />
	
	<!-- Note Service -->
	<bean id="noteDTOService" class="com.kiesoft.serviceimpl.note.DefaultNoteDTOService" />
	
</beans>